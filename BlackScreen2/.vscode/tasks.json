{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe x86 debug build",
      "command": "vsruncl.bat",
      "args": [
        "/x86",
        "/nologo",
        "/RTC1",
        "/Od",
        "/Oy-",
        "/MDd",
        "/D",
        "WIN32",
        "/D",
        "_DEBUG",
        "/D",
        "_UNICODE",
        "/D",
        "UNICODE",
        "/W4",
        "/Zc:inline",
        "/Zi",
        "/EHsc",
        "/Fd${workspaceFolderBasename}.pdb",
        "/Fe${workspaceFolderBasename}.exe",
        "*.c*",
        "/link",
        "/incremental:no"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "build",
      "detail": "compiler: cl.exe"
    },
